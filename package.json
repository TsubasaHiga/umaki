{
  "name": "umaki",
  "version": "0.7.2",
  "description": "A utility that is often used in web production.",
  "author": "TsubasaHiga",
  "homepage": "https://github.com/TsubasaHiga/umaki",
  "license": "MIT",
  "keywords": [
    "utility",
    "library"
  ],
  "packageManager": "pnpm@10.24.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/TsubasaHiga/umaki"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.es.js",
    "require": "./dist/index.cjs.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "analyze": "tsc && vite build --mode analyze",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "coverage": "vitest run --coverage",
    "lint": "biome lint",
    "format": "biome format"
  },
  "dependencies": {
    "@types/md5": "^2.3.6",
    "dayjs": "^1.11.19",
    "image-size": "^1.1.1",
    "isomorphic-dompurify": "^2.33.0",
    "md5": "^2.3.0",
    "query-string": "^9.3.1",
    "runes2": "^1.1.4",
    "store2": "^2.14.4",
    "ua-parser-js": "^1.0.39"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/node": "^24.10.1",
    "@types/ua-parser-js": "^0.7.39",
    "@vitest/coverage-v8": "4.0.15",
    "happy-dom": "^20.0.11",
    "jsdom": "^26.0.0",
    "rollup-plugin-license": "^3.6.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "~5.9.3",
    "vite": "^7.2.6",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15"
  }
}
